{
  "name": "@ifc/web",
  "description": "Forest City SDA church website",
  "version": "0.2.2",
  "private": true,
  "scripts": {
    "dev": "next dev -p $PORT",
    "build": "next build",
    "postbuild": "cp -r public .next/standalone && cp -r .next/static .next/standalone/.next && (cd .next/standalone && zip -r release.zip .)",
    "build:docker": "docker build --secret id=_env,src=.env -t cesosag/ifc-web .",
		"push:docker": "docker push cesosag/ifc-web",
    "start": "node server.js",
    "lint": "next lint",
    "test": "NODE_ENV=test jest",
    "test:watch": "jest --watch",
    "generate": "generate"
  },
  "dependencies": {
    "@next/font": "13.1.6",
    "axios": "1.3.4",
    "next": "13.1.6",
    "polished": "4.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.8.0",
    "react-is": "18.2.0",
    "sharp": "0.32.0",
    "socket.io": "4.6.1",
    "socket.io-client": "4.6.1",
    "styled-components": "5.3.9"
  },
  "devDependencies": {
    "@csjs/generate": "0.0.6",
    "@testing-library/dom": "9.2.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/gtag.js": "0.0.12",
    "@types/node": "18.15.11",
    "@types/react": "18.0.31",
    "@types/react-dom": "18.0.11",
    "@types/styled-components": "5.1.26",
    "@types/youtube": "0.0.47",
    "@typescript-eslint/eslint-plugin": "5.57.0",
    "@typescript-eslint/parser": "5.57.0",
    "eslint": "8.37.0",
    "eslint-config-next": "13.1.6",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "jest-styled-components": "7.1.1",
    "prettier": "2.8.7",
    "ts-node": "10.9.1",
    "typescript": "5.0.3"
  },
  "volta": {
    "node": "18.14.0",
    "yarn": "1.22.18"
  },
  "workspaces": {
    "nohoist": [
      "**"
    ]
