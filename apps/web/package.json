{
  "name": "@ifc/web",
  "description": "Forest City SDA church website",
  "version": "0.2.4",
  "private": true,
  "scripts": {
    "dev": "next dev -p $PORT",
    "build": "next build",
    "postbuild": "cp -r public .next/standalone && cp -r .next/static .next/standalone/.next && (cd .next/standalone && zip -r release.zip .)",
    "build:docker": "docker build --secret id=_env,src=.env -t ifcweb.azurecr.io/ifc-web .",
		"push:docker": "docker push ifcweb.azurecr.io/ifc-web",
    "start": "node server.js",
    "lint": "next lint",
    "lint-staged": "lint-staged",
    "test": "NODE_ENV=test jest",
    "test:watch": "jest --watch",
    "generate": "generate"
  },
  "dependencies": {
    "@next/font": "13.1.6",
    "axios": "1.4.0",
    "next": "13.1.6",
    "polished": "4.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.10.1",
    "react-is": "18.2.0",
    "sharp": "0.32.1",
    "socket.io": "4.7.0",
    "socket.io-client": "4.7.0",
    "styled-components": "5.3.11"
  },
  "devDependencies": {
    "@csjs/generate": "0.0.6",
    "@testing-library/dom": "9.3.1",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/gtag.js": "0.0.12",
    "@types/node": "20.3.1",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@types/styled-components": "5.1.26",
    "@types/youtube": "0.0.47",
    "@typescript-eslint/eslint-plugin": "5.60.0",
    "@typescript-eslint/parser": "5.60.0",
    "eslint": "8.43.0",
    "eslint-config-next": "13.1.6",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "jest-styled-components": "7.1.1",
    "prettier": "2.8.8",
    "ts-node": "10.9.1",
    "typescript": "5.1.3"
  },
  "volta": {
    "node": "18.14.0",
    "yarn": "1.22.18"
  },
  "workspaces": {
    "nohoist": [
      "**"
    ]
  }
